#include <iostream>

using namespace std;

int main()
{
    float num1, num2, soma, num3, mult;
    
    num1=10;
    
printf("O valor do munero 1 e: ");   

    printf("%f", num1);
   
    printf("\n Insira o numero 2: ");
    scanf("%f" , &num2);
    
    soma = num1 + num2;
   
    printf("\n Insira o numero 3: ");
    scanf("%f" , &num3);
   
    mult= soma  * num3;
    
   
    
    printf("\n O valor da multiplicação e: %f", mult);
    
    return 0;
}


